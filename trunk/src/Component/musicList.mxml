<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas xmlns:mx="http://www.adobe.com/2006/mxml" width="260" height="260" xmlns:Component="Component.*" verticalScrollPolicy="off">
	<Component:playerSongs id="l1"  y="0" doubleClickEnabled="true" styleName="player3" index="1"/>
	<Component:playerSongs2 id="l2" y="25" doubleClickEnabled="true" index="2"/>
	<Component:playerSongs id="l3"  y="50" doubleClickEnabled="true" index="3"/>
	<Component:playerSongs2 id="l4" y="75" doubleClickEnabled="true" index="4"/>
	<Component:playerSongs id="l5" y="100" doubleClickEnabled="true" index="5"/>
	<Component:playerSongs2 id="l6" y="125" doubleClickEnabled="true" index="6"/>
	<Component:playerSongs id="l7" y="150" doubleClickEnabled="true" index="7"/>
	<Component:playerSongs2 id="l8" y="175" doubleClickEnabled="true" index="8"/>
	<Component:playerSongs id="l9" y="200" doubleClickEnabled="true" index="9"/>
	<Component:playerSongs2 id="l10" y="225" doubleClickEnabled="true" index="10"/>
	
	<mx:Parallel id="listEffect">
		<mx:Move target="{l1}" yFrom="25" yTo="0" duration="500"/>
		<mx:Move target="{l2}" yFrom="50" yTo="25" duration="500"/>
		<mx:Move target="{l3}" yFrom="75" yTo="50" duration="500"/>
		<mx:Move target="{l4}" yFrom="100" yTo="75" duration="500"/>
		<mx:Move target="{l5}" yFrom="125" yTo="100" duration="500"/>
		<mx:Move target="{l6}" yFrom="150" yTo="125" duration="500"/>
		<mx:Move target="{l7}" yFrom="175" yTo="150" duration="500"/>
		<mx:Move target="{l8}" yFrom="200" yTo="175" duration="500"/>
		<mx:Move target="{l9}" yFrom="225" yTo="200" duration="500"/>
		<mx:Move target="{l10}" yFrom="250" yTo="225" duration="500"/>
	</mx:Parallel>
	
</mx:Canvas>
